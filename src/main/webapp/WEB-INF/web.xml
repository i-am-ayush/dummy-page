<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <listener>
    <listener-class>listener.OnStart</listener-class>
  </listener>

  <servlet>
    <servlet-name>RegisterSeeker</servlet-name>
    <servlet-class>registerseeker</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegisterSitter</servlet-name>
    <servlet-class>registersitter</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Logout</servlet-name>
    <servlet-class>logout</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>RegisterSeeker</servlet-name>
    <url-pattern>/registerseeker</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterSitter</servlet-name>
    <url-pattern>/registersitter</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
</web-app>
